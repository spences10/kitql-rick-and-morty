{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "$graphql": ["./src/lib/graphql"],
      "$graphql/*": ["./src/lib/graphql/*"],
      "$houdini": ["./$houdini"],
      "$houdini/*": ["./$houdini/*"]
    }
  }
}
